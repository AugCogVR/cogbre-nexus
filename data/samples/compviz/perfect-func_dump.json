            "type": "c",
            "id": "0",
            "code": {
                "0": "int perfect(int test)",
                "1": "{",
                "2": "     /* Utility counter and sum */",
                "3": "     int counter = 1;",
                "4": "     int sum = 0;",
                "5": "",
                "6": "     /* Count from 0 to test, adding all divisors of test to the sum */",
                "7": "     while (counter < test)",
                "8": "     {",
                "9": "          if (test % counter == 0)",
                "10": "               sum = sum + counter;",
                "11": "          counter++;",
                "12": "     }",
                "13": "",
                "14": "     /* If the sum == counter, this is a perfect number */",
                "15": "     return (sum == counter);",
                "16": "}"
            },
